    <div id="wrapper">
      <div id="main">
        <div class="inner">
          <section id="home-section">
            <div id="container02" class="container default full screen">
              <div class="wrapper">
                <div class="inner">
                  <h1 id="text07" class="style1">Nutrella</h1>
                  <p id="text15" class="style2">
                    Nutrella calulates your daily calorie intake and analyzes and then suggests meals that fit into your daily diet, based on the amount of calories your require.
                  </p>
                  <form action="#container05">
                    <div class="inner">
                      <div class="field"></div>
                      <div class="actions">
                        <%= link_to '#container06', class: "calculate", style: "padding: 20px; border-radius: 30px; text-decoration: none;" do %>
                          <span class="MuiButton-label"></span><span class="MuiTouchRipple-root">See Demo</span>
                        <% end %>
                      </div>
                    </div>
                    <input type="hidden" name="id" value="form02" />
                  </form>
                </div>
              </div>
            </div>

            <div id="container06" class="container default full screen">
              <div class="wrapper">
                <div class="inner">
                  <h2 id="text28" class="style3">Calculate your energy needs</h2>
                  <p id="text29" class="style5">Fill in the fields below and get the 4 time meals curated according to the calories do you need in a day</p>
                  <%= render "calculator" %>
                </div>
              </div>
            </div>
            
            <div id="container05" class="container columns full screen">
              <div class="meal-wrapper wrapper" style="padding: 7rem 4rem;">
                <!-- Dynamic Data -->
                <div class="loader"></div>
              </div>
            </div>

            <hr id="divider02">
            <div id="container28" class="style1 container default full screen">
              <div class="wrapper">
                <div class="inner">
                  <h2 id="text14" class="style1">What we do!</h2>
                  <p id="text03">Nutrella is the simplest way to control the calories you intake. Never count calories again, Nutrella does it all for you. Add your breakfast, lunch, and dinner and Nutrella provides how many more meals you can eat of a desired food type.</p>
                </div>
              </div>
            </div>
          <section id="done-section">
            <div id="container36" class="container default full screen">
              <div class="wrapper">
                <div class="inner">
                  <h3 id="text01" class="style4">Nutrella</h3>
                  <h2 id="text12">Thank you!</h2>
                  <p id="text09" class="style5">Thanks for using our tool!</p>
                  <ul id="icons03" class="style1 icons">
                    <li>
                      <a class="n01" href="https://domain.ext/path">
                        <svg>
                          <use xlink:href="#icon-905"></use>
                        </svg>
                        <span class="label">Twitter</span>
                      </a>
                    </li>
                    <li>
                      <a class="n02" href="https://domain.ext/path">
                        <svg>
                          <use xlink:href="#icon-910"></use>
                        </svg>
                        <span class="label">Instagram</span>
                      </a>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
          </section>
        </div>
      </div>
    </div>


<script type="text/javascript">
  $(".calculate").on('click', function(){
    $(".meal-wrapper.wrapper").html('<div class="loader"></div>');
    $(".loader").show();
     $('html, body').animate({
        scrollTop: $(".meal-wrapper").offset().top
    }, 1000);
    $.get('/fetch_meals', function(res){})
    return false;
  })
</script>